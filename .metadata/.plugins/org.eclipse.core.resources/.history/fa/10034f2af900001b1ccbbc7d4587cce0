package lambdas;

public class CalculoTeste2 {
	public static void main(String[] args) {
		Calculo calculo = (double a, double b) -> { return a + b; };
		System.out.println(calculo.executar(3, 5));
		
		Calculo calculo2;
		calculo = (x, y) -> { return x + y; };
		System.out.println(calculo.executar(6, 6));
		
		calculo = (x, y) -> x * y;
		System.out.println(calculo.executar(4, 4));
	}
}

package Classe;

interface GenericProduto {
	String nome = "";
	double preco = 0;
	double desconto = 0;
	double aplicarDesconto (double descontoGerente);
}

public class Produto implements GenericProduto {

	String nome;
	double preco;
	double desconto;
	public double aplicarDesconto (double descontoGerente) {
		return this.preco * (1 - this.desconto);
	}
	
}
